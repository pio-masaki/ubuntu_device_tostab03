#!/system/bin/sh
#Jacob Holtom - 03/02/2013 for Ubuntu Touch on TF101 - automatic resolution fix - adapted to tostab03 by pio_masaki - I left this in after he removed it just to give him his credit

device="/data/ubuntu/etc/ubuntu-session.d/tostab03.conf"


if [ ! -f $device ]
then
echo "installing device file"
  cp /system/etc/ubuntu-session.d/tostab03.conf $device 
fi

